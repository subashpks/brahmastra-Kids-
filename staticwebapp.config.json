{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"],
      "headers": {
        "X-Robots-Tag": "noindex, nofollow"
      }
    }
  ],
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
